{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./Actions/NodeActions.ts",
        "./Stores/NodeStore.ts",
        "./TestView/TestView.ts",
        "./appView/appView.ts",
        "./main.ts"
    ],
    "compileOnSave": false
}
